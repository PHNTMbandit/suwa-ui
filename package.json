{
  "name": "strata-ui-react",
  "private": false,
  "version": "0.1.10",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && pnpm run build:css",
    "build:css": "npx @tailwindcss/cli -i ./src/style.css -o ./dist/style.css --minify",
    "lint": "eslint .",
    "preview": "vite preview",
    "release:patch": "pnpm run build && pnpm version patch && git push && pnpm publish",
    "release:minor": "pnpm run build && pnpm version minor && git push && pnpm publish",
    "release:major": "pnpm run build && pnpm version major && git push && pnpm publish"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css",
    "./style.css": "./dist/style.css"
  },
  "style": "./dist/style.css",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@phosphor-icons/react": "^2.1.10",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-form": "^1.27.6",
    "@tanstack/react-ranger": "^0.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/cli": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  }
}
