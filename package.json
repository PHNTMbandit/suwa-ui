{
	"name": "strata-ui-react",
	"version": "0.2.11",
	"type": "module",
	"author": "PHNTMbandit",
	"repository": {
		"type": "git",
		"url": "https://github.com/PHNTMbandit/strata-ui-react"
	},
	"main": "./dist/strata-ui-react.es.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/strata-ui-react.es.js"
		},
		"./styles": "./dist/styles.css"
	},
	"files": [
		"dist"
	],
	"sideEffects": [
		"**/*.css"
	],
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && rm -rf ./dist && vite build  && pnpm run build:css",
		"build:css": "npx tailwindcss -i ./src/styles.css -o ./dist/styles.css --minify",
		"lint": "biome check --write",
		"preview": "vite preview",
		"test": "vitest --passWithNoTests",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"prepare": "husky",
		"release": "standard-version"
	},
	"dependencies": {
		"@tailwindcss/cli": "^4.1.18",
		"@tailwindcss/vite": "^4.1.18",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.18"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@chromatic-com/storybook": "^4.1.3",
		"@commitlint/cli": "^20.2.0",
		"@commitlint/config-conventional": "^20.2.0",
		"@phosphor-icons/react": "^2.1.10",
		"@storybook/addon-a11y": "^10.1.10",
		"@storybook/addon-docs": "^10.1.10",
		"@storybook/addon-onboarding": "^10.1.10",
		"@storybook/addon-themes": "^10.1.10",
		"@storybook/addon-vitest": "^10.1.10",
		"@storybook/react-vite": "^10.1.10",
		"@tanstack/react-form": "^1.27.6",
		"@tanstack/react-ranger": "^0.0.5",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/react": "^16.3.1",
		"@types/node": "^25.0.3",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/browser-playwright": "^4.0.16",
		"@vitest/coverage-v8": "^4.0.16",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"jsdom": "^27.4.0",
		"playwright": "^1.57.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-test-renderer": "^19.2.3",
		"standard-version": "^9.5.0",
		"storybook": "^10.1.10",
		"typescript": "~5.9.3",
		"vite": "npm:rolldown-vite@7.3.0",
		"vite-plugin-dts": "^4.5.4",
		"vitest": "^4.0.16"
	},
	"peerDependencies": {
		"@base-ui/react": "^1.0.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"pnpm": {
		"overrides": {
			"vite": "npm:rolldown-vite@7.3.0"
		}
	}
}
