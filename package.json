{
  "name": "strata-ui-react",
  "description": "A set of accessible, customizable, and production-ready React components built with Tailwind CSS.",
  "private": false,
  "version": "0.1.12",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.ts"
      }
    },
    "./styles.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b tsconfig.build.json && vite build && pnpm run build:css",
    "build:css": "npx @tailwindcss/cli -i ./src/style.css -o ./dist/style.css --minify",
    "lint": "eslint .",
    "preview": "vite preview",
    "release:patch": "pnpm run build && pnpm version patch && git push && pnpm publish",
    "release:minor": "pnpm run build && pnpm version minor && git push && pnpm publish",
    "release:major": "pnpm run build && pnpm version major && git push && pnpm publish"
  },
  "devDependencies": {
    "@base-ui/react": "^1.0.0",
    "@eslint/js": "^9.39.2",
    "@phosphor-icons/react": "^2.1.10",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-form": "^1.27.6",
    "@tanstack/react-ranger": "^0.0.5",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rollup-plugin-node-externals": "^8.1.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.2.5"
    }
  }
}
